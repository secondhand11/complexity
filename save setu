#include<bits/stdc++.h>
using namespace std;

int main (){
	int n,j=0;
	long hasil=0;
	long c[100];
	cin >> n;
	long a;
	string b;
	for (int i=0;i<n;i++){
		cin >> b;
		if (b=="donate"){
			cin >> a;
			hasil = hasil+a;
		}
		else {
			c[j]=hasil;
			j++;
		}
	}
	for (int i=0;i<j;i++){
		if (i<j-1){
			cout << c[i] <<endl;
		}
		else {
			cout << c[i];
			
		}
	}
}
