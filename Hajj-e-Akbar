#include<bits/stdc++.h>
using namespace std;

int main (){
	string x;
	string result[20];
	int i=0;
	do{
		cin >> x;
		if (x=="Hajj"){
			result[i]="Hajj-e-Akbar";
		}
		else {
			result[i]="Hajj-e-Asghar";
		}
		i++;
	}
	while(x!="*");
		
	int j=i-1;
	i=0;
	for (int i=0;i<j;i++){
		if(i<j-1){
		cout << "Case "<<i+1<<": "<<result[i]<<endl;
			
		}
		else{
			cout << "Case "<<i+1<<": "<<result[i];
			}
	}
	
}
